page{
  background: rgb(228, 227, 227);
}
.itemWrap{
  background: #fff;
  width: 100%;
  margin-bottom: 20rpx;
  padding: 0 30rpx;
  border-radius: 16rpx;
}
.profileWrap{
  padding: 0 20rpx;
}
.profileInfo{
  height: 100rpx;
  align-items: center;
  display: flex;
  justify-content: space-between;
}
.img{
  display: flex;
}
.photo{
  height: 90rpx;
  width: 90rpx;
  background: #666;
}
.name{
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 10rpx;
}
.userName{
  font-size: 30rpx;
}
.userScore{
  font-size: 26rpx;
  color: #666;
}
.btn{
  background: #fff;
  font-size: 20rpx;
  font-weight: 400;
  color: rgb(32, 32, 214);
}
.reflash{
  font-size: 26rpx;
  color: rgb(32, 32, 214);
}
.vipNumb{
  height: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.vipTitle{
  display: flex;
  justify-content: center;
}
.writeBtn{
  background: #fff;
}
.titleTop{
  font-size: 26rpx;
  color: #666;
}
.vipContent{
  font-size: 50rpx;
  position: relative;
}
.copyNum{
  position: absolute;
  right: 0;
  font-size: 16rpx;
}
.range, .play, .scoreFlow{
  height: 100rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #ccc;
}
.range, .play, .scoreFlow, .qrcode{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.qrcode{
  height: 100rpx;
  box-sizing: border-box;
}